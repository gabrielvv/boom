{
  "name": "boom",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:api\"",
    "dev:api": "nodemon --watch",
    "dev:app:cp": "mkdir -p ../demo/dev && cp ../demo/index.html ../demo/dev/index.html",
    "dev:app:sed": "sed -i -e \"s/API_BASE_URL/http:\\/\\/localhost:3000/\" -e \"s/BASE_PATH/\\//\" ../demo/dev/index.html",
    "dev:app:serve": "http-serve ../demo/dev",
    "dev:app": "npm run dev:app:cp && npm run dev:app:sed && npm run dev:app:serve",
    "lint": "eslint --cache --fix ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.662.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.4.0",
    "helmet": "^4.1.0",
    "jwks-rsa": "^1.8.0",
    "lodash": "^4.17.15",
    "redis": "^3.0.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "http-serve": "^1.0.1",
    "husky": "^4.2.3",
    "jest": "^26.0.1",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add",
      "jest --findRelatedTests"
    ]
  }
}
